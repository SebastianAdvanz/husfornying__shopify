{%- assign headerSnippetPath = "header-pagestyle-guide" -%}{%- assign wfPage = "6290a59843dad598484fab2d" -%}{%- assign bodyClasses = "body" -%}
  <div class="section-header wf-section">
    {%- render 'navbar' -%}
  </div>
  <div class="section-padding wf-section">
    <div class="reviews-discover">
      <div class="content-top-rhead center">
        <div class="red-lines-header">
          <div class="line"></div>
          <p class="_18bi-caps-kanit">{{ 'tn3bf12d97' | t }}</p>
          <div class="line"></div>
        </div>
        <h1 class="h2-blue center bottom20">{{ article.title }}</h1>
        <div class="_20sb-martel light">{{ article.created_at | date: "%e/%m/%Y" }}</div>
      </div>
    </div>
  </div>
  <div class="wf-section">
    <div class="diy-hero-img" style="background-image: url('{{article.metafields.udesly['hero-image'].src}}')"></div>
  </div>
  <div class="section-padding wf-section">
    <div class="diy-rich-content">
      <div class="socialeft">
        <div class="_16reg mclear mbottom10">{{ 't3edc9a0' | t }}</div>
        <a href="#" class="link-block w-inline-block"><img src="{{ 'icon.svg' | asset_url }}" loading="lazy" alt=""></a>
        <a href="#" class="link-block w-inline-block"><img src="{{ 'icon-1.svg' | asset_url }}" loading="lazy" alt=""></a>
        <a href="#" class="link-block w-inline-block"><img src="{{ 'icon-2.svg' | asset_url }}" loading="lazy" alt=""></a>
        <a href="#" class="link-block w-inline-block"><img src="{{ 'icon-3.svg' | asset_url }}" loading="lazy" alt=""></a>
      </div>
      <div class="diy-richtext">
        <div class="w-richtext">{{ article.content }}</div>
      </div>
      <div class="diy-author"><img src="{{ article.metafields.udesly['photo-author'].src }}" loading="lazy" alt="" class="image-57">
        <div class="diy-info-author">
          <div class="_20eb">{{ article.metafields.udesly['name-author'] }}</div>
          <div class="_14reg blue martel">{{ article.metafields.udesly['position-author'] }}</div>
        </div>
      </div>
    </div>
  </div>
  <div class="section-padding wf-section">
    <div class="diy-small-photo">
      {%- assign setItems = article.metafields.udesly['small-photo'] -%}<div class="w-dyn-list">
        {%- unless setItems.size == 0 -%}<div role="list" class="collection-list-27 w-dyn-items">
          {%- for setItem in setItems -%}<div role="listitem" class="collection-item-25 w-dyn-item">
            <a href="#" class="w-inline-block w-lightbox"><img src="{{ setItem.src }}" loading="lazy" alt="">
              <script type="application/json" class="w-json">{"items":[],"group":""}</script>
            </a>
          </div>{%- endfor -%}
        </div>{%- endunless -%}
        {%- if setItems.size == 0 -%}<div class="w-dyn-empty">
          <div>{{ 't3d503d2b' | t }}</div>
        </div>{%- endif -%}
      <template><div class="w-dyn-list" udesly-repeater="true">
        <div role="list" class="collection-list-27 w-dyn-items">
          <div role="listitem" class="collection-item-25 w-dyn-item">
            <a href="#" class="w-inline-block w-lightbox"><img src="{# setItem.src #}" loading="lazy" alt="">
              <script type="application/json" class="w-json">{"items":[],"group":""}</script>
            </a>
          </div>
        </div>
        <div class="w-dyn-empty">
          <div>No items found.</div>
        </div>
      </div></template></div>
    </div>
  </div>
  <div class="section-padding wf-section">
    <div class="diy-rich-content-story2">
      <div class="diy-richtext-story2">
        <div class="w-richtext">{{ article.metafields.udesly['story-content-2'] }}</div>
      </div>
    </div>
  </div>
  <div class="section-padding wf-section">
    <div class="quote">
      <div class="diy-grey-quote"><img src="{{ '.svg' | asset_url }}" loading="lazy" alt="" class="image-53">
        <div class="_20sb-martel center">{{ article.metafields.udesly['quote'] }}</div>
        <div class="_18bi-caps-kanit center mtop20">{{ article.metafields.udesly['quote-author-s-name'] }}</div>
      </div>
    </div>
  </div>
  <div class="section-padding wf-section">
    <div class="diy-large-photo">
      {%- assign setItems = article.metafields.udesly['large-photo'] -%}<div class="w-dyn-list">
        {%- unless setItems.size == 0 -%}<div role="list" class="collection-list-28 w-dyn-items">
          {%- for setItem in setItems -%}<div role="listitem" class="w-dyn-item">
            <a href="#" class="w-inline-block w-lightbox"><img src="{{ setItem.src }}" loading="lazy" alt="">
              <script type="application/json" class="w-json">{"items":[{"type":"image","url":"{{ setItem.src }}","caption":""}],"group":""}</script>
            </a>
          </div>{%- endfor -%}
        </div>{%- endunless -%}
        {%- if setItems.size == 0 -%}<div class="w-dyn-empty">
          <div>{{ 't3d503d2b' | t }}</div>
        </div>{%- endif -%}
      <template><div class="w-dyn-list" udesly-repeater="true">
        <div role="list" class="collection-list-28 w-dyn-items">
          <div role="listitem" class="w-dyn-item">
            <a href="#" class="w-inline-block w-lightbox"><img src="{# setItem.src #}" loading="lazy" alt="">
              <script type="application/json" class="w-json">{"items":[{"type":"image","url":"{# setItem.src #}","caption":""}],"group":""}</script>
            </a>
          </div>
        </div>
        <div class="w-dyn-empty">
          <div>No items found.</div>
        </div>
      </div></template></div>
    </div>
  </div>
  <div class="section-padding wf-section">
    <div class="diy-rich-content-story2">
      <div class="diy-richtext-story2">
        <div class="w-richtext">{{ article.metafields.udesly['story-content-3'] }}</div>
      </div>
    </div>
  </div>
  <div class="diy-rich-content-mobile">
    <div class="diy-author-mobile"><img src="{{ article.metafields.udesly['photo-author'].src }}" loading="lazy" alt="" class="image-57">
      <div class="diy-info-author">
        <div class="_20eb">{{ article.metafields.udesly['name-author'] }}</div>
        <div class="_14reg blue martel">{{ article.metafields.udesly['position-author'] }}</div>
      </div>
    </div>
    <div class="socialeft-mobile">
      <div class="_16reg mclear mbottom10">{{ 't3edc9a0' | t }}</div>
      <a href="#" class="link-block w-inline-block"><img src="{{ 'icon.svg' | asset_url }}" loading="lazy" alt=""></a>
      <a href="#" class="link-block w-inline-block"><img src="{{ 'icon-1.svg' | asset_url }}" loading="lazy" alt=""></a>
      <a href="#" class="link-block w-inline-block"><img src="{{ 'icon-2.svg' | asset_url }}" loading="lazy" alt=""></a>
      <a href="#" class="link-block w-inline-block"><img src="{{ 'icon-3.svg' | asset_url }}" loading="lazy" alt=""></a>
    </div>
  </div>
  <div class="section-padding wf-section">
    <div class="diy-tools">
      <h3 class="h3-blue center">{{ 'tn156ab596' | t }}</h3>
      <div class="_18reg martel center">{{ 'tn515ae3b9' | t }}</div>
      {%- if settings.collection_diy_stories_template != blank -%}{%- render "markup_for_collection_diy_stories_template" -%}{%- else -%}{%- render "products_max_5" -%}{%- endif -%}
    </div>
    <div class="diy-information">
      <div class="diy-info">
        <div class="_16reg">{{ 'tn3bfa92cf' | t }}</div>
      </div>
      <div class="diy-total-cost-cta">
        <div class="diy-total-cost">
          <div class="_16reg caps mbottom10">{{ 'tn677f446a' | t }}</div>
          <div class="diy-cost">
            <h3 class="h3-blue">{{ 'tn56dd87d6' | t }}</h3>
            <div class="_20sb-martel light crossedout">{{ 't43d7333a' | t }}</div>
          </div>
        </div>
        <a href="#" class="button-14 w-button">{{ 't7369b832' | t }}</a>
      </div>
    </div>
  </div>
  <div class="section-wave wf-section">
    <div class="wave rose"><img src="{{ 'wave-rose.png' | asset_url }}" loading="lazy" sizes="100vw" srcset="{{ 'wave-rose-p-500.png' | asset_url }} 500w, {{ 'wave-rose-p-800.png' | asset_url }} 800w, {{ 'wave-rose-p-1080.png' | asset_url }} 1080w, {{ 'wave-rose-p-1600.png' | asset_url }} 1600w, {{ 'wave-rose-p-2000.png' | asset_url }} 2000w, {{ 'wave-rose-p-2600.png' | asset_url }} 2600w, {{ 'wave-rose.png' | asset_url }} 3840w" alt="" class="image-23"></div>
  </div>
  <div class="section-cta-copy wf-section">
    <div class="bg-wave-rose">
      <div class="content-cta-diy">
        <h3 class="h3-blue center">{{ 't10942a67' | t }}</h3>
        <fieldset class="w-commerce-commercecheckoutblockcontent block-content-diy">
          <div class="w-form">
            {%- form "contact",  class: "" -%}<label for="contact[email]">{{ 't3ff5b7c' | t }}</label><input type="email" name="contact[email]" class="w-input" required="">
              <div class="_16reg mbottom10">{{ 't7bec1424' | t }}<span class="text-span-11">{{ 't2a' | t }}</span></div>
              <div class="div-block-61"><input type="text" class="w-commerce-commercecheckoutemailinput email-input-diy" name="contact[email]" required="">
                <a href="#" class="button-cta-diy w-button">{{ 'tn6a687fe4' | t }}</a>
              </div>
            {%- assign form_success = form.posted_successfully? -%}{%- assign form_errors = form.errors -%}{%- render "form-general-script", form_success: form_success, form_errors: form_errors -%}{%- endform -%}
            <div class="w-form-done  {% if form_success %}form-posted-successfully{% endif %}">
              <div>{{ 't5a0ea5a1' | t }}</div>
            </div>
            {%- unless form_success -%}<div class="w-form-fail  {% if form.errors %}form-has-errors{% endif %}">
              <div>{%- render "form-errors", form_errors: form_errors -%}</div>
            </div>{%- endunless -%}
          </div>
        </fieldset>
      </div>
    </div>
  </div>
  {%- render 'footer' -%}
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=6287bb88d050e6c7bbe247e8" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->

